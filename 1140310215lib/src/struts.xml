<struts>
    <package name="wwfy" extends="struts-default">
        <action name="loginUser" class="wwfy.user.action.UserAction">
            <result name="success">/ind.jsp</result>
            <result name="error">/register.jsp</result>
        </action>
        <action name="bookinf" class="wwfy.user.action.wdAction">
            <result name="success">/inf.jsp</result>
        </action>
        <action name="bookde" class="wwfy.user.action.wdeleAction">
            <result name="desuccess">/desuccess.jsp</result>
        </action>
    </package>
</struts>